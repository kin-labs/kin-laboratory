      ],
      "@kin-laboratory/web/core/data-access": [
        "libs/web/core/data-access/src/index.ts"
      ],
      "@kin-laboratory/web/core/feature": [
        "libs/web/core/feature/src/index.ts"
      ],
      "@kin-laboratory/web/home/feature": [
        "libs/web/home/feature/src/index.ts"
      ],
      "@kin-laboratory/web/keypair/feature": [
        "libs/web/keypair/feature/src/index.ts"
      ],
      "@kin-laboratory/web/memo/feature": [
        "libs/web/memo/feature/src/index.ts"
